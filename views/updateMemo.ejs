    <title>Document</title>
</head>
<body>
  
    <a href="/homepage">To homepage</a>

  
    <form method="POST" action="/memo/createMemo" >
      <div>
        <label>Title</label>
        <input name="title" type="text" value="<%= memo[0].title %>" >
      </div>

      <div>
        <label>Content</label>
        <textarea name="content" rows="20" type="textArea"> <%= memo[0].title %> </textArea>
      </div>

      <button>Submit</button>
    </form> 
  </nav>
